{
    "env": {
        "browser": true,
        "es2021": true,
		"jest": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "linebreak-style":0,
        "react/prop-types":0,
        "react/destructuring-assignment":0,
        "arrow-body-style":0,
        "no-unused-vars":0,
        "react/button-has-type":0,
        "quotes":0,
        "import/prefer-default-export":0,
        "object-shorthand":0,
        "max-classes-per-file":0,
        "no-useless-constructor":0,
        "react/no-unused-state":0
    },
    "overrides": [
    {
        "files": [
        "**/*.test.js",
        "**/*.test.jsx"
        ]
    }]
}
